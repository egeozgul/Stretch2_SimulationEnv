<mujoco model="throwing_environment">
  <compiler angle="radian" assetdir="." autolimits="true"/>
  
  <option timestep="0.002" gravity="0 0 -9.81" integrator="RK4"/>
  
  <visual>
  <quality shadowsize="0" /> <global offwidth="640" offheight="480" /> </visual>

  <asset>
    <mesh name="tomato" file="../meshes/tomato.obj" scale="0.06 0.06 0.06"/>
    <texture name="tomato_tex" type="2d" file="../textures/material0_baseColor.png"/>
    <material name="tomato_mat" texture="tomato_tex"/>
    <mesh name="onion" file="../meshes/onion.obj" scale="0.013 0.013 0.013"/>
    <texture name="onion_tex" type="2d" file="../textures/Red_onion_diffuse.png"/>
    <material name="onion_mat" texture="onion_tex"/>
    <mesh name="plate" file="../meshes/plate.obj"/>
    <mesh name="bell" file="../meshes/bell.obj" />
    <texture name="bell_tex" type="2d" file="../textures/Material.001_baseColor.png"/>
    <material name="bell_mat" texture="bell_tex"/>
    <mesh name="table" file="../meshes/table_fixed.obj" scale="1 1 0.55"/>
    <texture name="table_tex" type="2d" file="../textures/Material.001_baseColor.png"/>
    <material name="table_mat" texture="table_tex"/>
    
<!-- Change this in your <asset> section -->
    <texture name="texplane" type="2d" builtin="flat" rgb1="0.7 0.7 0.7" width="512" height="512"/>
    <material name="matplane" texture="texplane" texuniform="true" reflectance="0.3"/>
    <!-- Sky -->
    <texture type="skybox" builtin="gradient" rgb1="0.4 0.6 0.8" rgb2="0.1 0.2 0.3" width="512" height="3072"/>
    
    
    <!-- Wood texture for tables -->
    <texture name="texwood" type="2d" builtin="flat" rgb1="0.6 0.4 0.2" width="256" height="256"/>
    <material name="matwood" texture="texwood" reflectance="0.1"/>
    
    <!-- Plate texture -->
    <texture name="texplate" type="2d" builtin="flat" rgb1="0.95 0.95 0.95" width="256" height="256"/>
    <material name="matplate" texture="texplate" reflectance="0.3"/>
  </asset>
  <include file="stretch.xml"/>
  <worldbody>
    <site name="anchor_A" pos="-0.65 1 0.00" size="0.15" rgba="0 1 0 0.5" type="sphere"/>
    
    <site name="anchor_B" pos="0.65 1 0.00" size="0.15" rgba="1 0 0 0.5" type="sphere"/>

    <site name="anchor_C" pos="-1.0 5 0.00" size="0.15" rgba="1 0 0 0.5" type="sphere"/>

    <site name="anchor_D" pos="1.0 5 0.00" size="0.15" rgba="1 0 0 0.5" type="sphere"/>

    <!-- Lighting for large space -->
    <light directional="true" diffuse="0.8 0.8 0.8" specular="0.3 0.3 0.3" pos="0 0 10" dir="0 0 -1" castshadow="true"/>
    <light directional="true" diffuse="0.4 0.4 0.4" specular="0.1 0.1 0.1" pos="10 10 8" dir="-1 -1 -1"/>
    <light directional="true" diffuse="0.4 0.4 0.4" specular="0.1 0.1 0.1" pos="-10 10 8" dir="1 -1 -1"/>

    <!-- Large ground plane -->
    <geom name="floor" type="plane" size="50 50 0.1" material="matplane" condim="3" friction="1 0.5 0.5"/>

    <body name="wall_left" pos="-8 0 1.5">
     <geom name="wall_left_geom" type="box" size="0.1 8 1.5" rgba="0.9 0.9 0.85 1" friction="0.8 0.5 0.5"/>
    </body>

<!-- Wall 2: Back wall (parallel to X-axis) -->
    <body name="wall_back" pos="0 -8 1.5">
      <geom name="wall_back_geom" type="box" size="8 0.1 1.5" rgba="0.9 0.9 0.85 1" friction="0.8 0.5 0.5"/>
    </body>
    <!-- TABLE 1: Robot side (at origin) -->
    <body name="table1" pos="0 0 0">
      <geom name="table1_geom" type="mesh" mesh="table" material="table_mat" friction="0.8 0.5 0.5"/><!--columb friction coeff (tangential,rotational, rolling)-->
    </body>

    <body name="plate1" pos="-0.65 0.37 1.01">
      <joint type="free" damping="0.01"/>
      <geom name="plate1_geom" type="mesh" mesh="plate" mass="0.1"friction="0.6 0.4 0.4" solref="0.01 1.0" solimp="0.99 0.99 0.001"/>
    </body>

    <body name="plate2" pos="0.65 0.37 1.01">
      <joint type="free" damping="0.2"/>
      <geom name="plate2_geom" type="mesh" mesh="plate" mass="0.1"friction="0.6 0.4 0.4" solref="0.01 1.0" solimp="0.99 0.99 0.001"/>
    </body>

    
    <!-- table 2 elements-->
    <body name="onion1" pos="-1.0 5.42 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="onion1_geom" type="mesh" mesh="onion" material="onion_mat" mass="0.15" friction="1.5 0.8 0.8"/>
      <site name="onion1_site" size="0.01" rgba="1 1 0 1"/>
    </body>
    
    <body name="onion2" pos="-0.85 5.42 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="onion2_geom" type="mesh" mesh="onion" material="onion_mat" mass="0.15" friction="1.5 0.8 0.8"/>
      <site name="onion2_site" size="0.01" rgba="1 1 0 1"/>
    </body>
    
    <body name="onion3" pos="-1.0 5.54 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="onion3_geom" type="mesh" mesh="onion" material="onion_mat" mass="0.15" friction="1.5 0.8 0.8"/>
      <site name="onion3_site" size="0.01" rgba="1 1 0 1"/>
    </body>

    <body name="tomato1" pos="1.0 5.42 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="tomato1_geom" type="mesh" mesh="tomato" material="tomato_mat" mass="0.13" friction="1.5 0.8 0.8"/>
      <site name="tomato1_site" size="0.01" rgba="1 0 0 1"/>
    </body>
    
    <body name="tomato2" pos="0.85 5.42 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="tomato2_geom" type="mesh" mesh="tomato" material="tomato_mat" mass="0.13" friction="1.5 0.8 0.8"/>
      <site name="tomato2_site" size="0.01" rgba="1 0 0 1"/>
    </body>
    
    <body name="tomato3" pos="1.0 5.54 1.05">
      <joint type="free" damping="0.001"/>
      <geom name="tomato3_geom" type="mesh" mesh="tomato" material="tomato_mat" mass="0.13" friction="1.5 0.8 0.8"/>
      <site name="tomato3_site" size="0.01" rgba="1 0 0 1"/>
    </body>


    <body name="bell1" pos="-0.1 5.42 1.02">
      <joint type="free" damping="0.001"/>
      <geom name="bell1_geom" type="mesh" mesh="bell" material="bell_mat" mass="0.11" friction="1.5 0.8 0.8"/>
      <site name="bell1_site" size="0.01" rgba="0 1 0 1"/>
    </body>

    <body name="bell2" pos="0.1 5.42 1.02">
      <joint type="free" damping="0.001"/>
      <geom name="bell2_geom" type="mesh" mesh="bell" material="bell_mat" mass="0.11" friction="1.5 0.8 0.8"/>
      <site name="bell2_site" size="0.01" rgba="0 1 0 1"/>
    </body>

    <body name="bell3" pos="-0.1 5.54 1.02">
      <joint type="free" damping="0.001"/>
      <geom name="bell3_geom" type="mesh" mesh="bell" material="bell_mat" mass="0.11" friction="1.5 0.8 0.8"/>
      <site name="bell3_site" size="0.01" rgba="0 1 0 1"/>
    </body>


    <body name="table2" pos="0 6 0">
      <geom name="table2_geom" type="mesh" mesh="table" material="table_mat" friction="0.8 0.5 0.5"/>
    </body>
    
    
    <camera name="side_view" pos="4 -8 3" xyaxes="1 0 0 0 0.5 1" mode="fixed"/>
    <camera name="top_view" pos="4 0 15" xyaxes="1 0 0 0 1 0" mode="fixed"/>
    <camera name="robot_view" pos="-2 -3 2" xyaxes="0.8 0.6 0 -0.3 0.4 1" mode="fixed"/>
    <camera name="target_view" pos="8 -5 3" xyaxes="1 0 0 0 0.6 1" mode="fixed"/>
    <camera name="table1_view" pos="0 -2 2" xyaxes="1 0 0 0 0.7 1" mode="fixed"/>
  </worldbody>
</mujoco>